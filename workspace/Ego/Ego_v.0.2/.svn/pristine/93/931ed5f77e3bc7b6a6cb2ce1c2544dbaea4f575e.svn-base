package ego.vo;

public class BoardVO
{
	private String no;      //게시물 번호
	private String u_no;    //회원 번호
	private String u_nick;  //회원 닉네임
	private String title;   //게시물 제목
	private String date;    //게시물 작성일
	private String note;    //게시물 내용
	private String kind;    //작성글 종류
	
	private String ansnote; //답변 내용
	private String ansdate; //답변 일자
	
	
	
	public String getNo()      { return no; 	 }
	public String getUno()     { return u_no;	 }
	public String getUnick()   { return u_nick;	 }
	public String getTitle()   { return title;	 }
	public String getDate()    { return date;	 }
	public String getNote()    { return note;	 }
	public String getKind()    { return kind;	 }
	public String getAnsnote() { return ansnote; }
	public String getAnsdate() { return ansdate; }
	
	public String getKindName()
	{
		if(kind.equals("N"))
		{
			return "공지사항";
		}else
		{
			return "문의사항";
		}
	}
	
	public void setNo(String no)            { this.no      = no;	  }
	public void setUno(String u_no) 		{ this.u_no    = u_no;	  }
	public void setUnick(String u_nick) 	{ this.u_nick  = u_nick;  }
	public void setTitle(String title) 		{ this.title   = title;   }
	public void setDate(String date) 		{ this.date    = date;	  }	
	public void setNote(String note) 		{ this.note    = note;	  }
	public void setKind(String kind) 		{ this.kind    = kind;	  }
	public void setAnsnote(String ansnote)  { this.ansnote = ansnote; }
	public void setAnsdate(String ansdate)  { this.ansdate = ansdate; }
	
}
