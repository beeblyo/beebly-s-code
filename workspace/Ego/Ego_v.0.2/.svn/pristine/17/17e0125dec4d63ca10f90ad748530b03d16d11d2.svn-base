/*********************************************************** 공통작업 ***********************************************************/	
header
{
	width: 100%;
	height: 80px;
	background: #fff;
	position: fixed;
	z-index: 99;
	top: 0;
	left: 0;
}

header > div
{
	width: 1100px;
	margin: 0 auto;
	position: relative;
}

h1 
{
	display: inline-block;
	width:140px;
	height:60px;
	position: absolute;
	top: 15px;
}

h1 a
{
	display: inline-block;
	width: 100%;
	height: 100%;
}

.login_wrap 
{
	display: inline-block;
	background: #fff;
	
	position: absolute;
	right: 0;
	top: 35px;
}

.login_wrap a
{
	width: 30px;
	height: 30px;
	border: 2px solid #282828;
	border-radius: 20px;
	background: url("/Ego_v.0.2/img/icon/info.jpg") no-repeat center center;
	background-size: cover;
}

.login_wrap a:nth-child(2)
{
	border: 0;
	border-radius: 0;
	background: url("/Ego_v.0.2/img/icon/logout.png") no-repeat center center;
	background-size: cover;
	margin-left: 10px;
	margin-bottom: 2px;
}

.notice_wrap 
{
	display: inline-block;
	font-size: 14px;
	
	position: absolute;
	right: 0;
	top: 0;
}


.search 
{
	width: 600px;
	height: 40px;
	background: #fff;
	position:absolute;
	
	left: 50%;
  	transform: translate(-50%, 50%);
}

.search input[type=text]
{
	width: 475px;
	padding: 12px;
	border: 1px solid #ddd;
	float:left;
}


.search_select
{
	width: 85px;
	padding: 0 10px;
	height:40px;
	float:left;
}

.search_btn
{
	width:40px;
	height:100%;
	padding: 5px;
	outline:none;
	float:right;
}

.recent
{
	display: none;
	width: 100%;
    background: #fff;
    position: fixed;
    top: 80px;
    left: 0;
    box-shadow: 0 5px 5px rgba(0,0,0,0.3);
    z-index: 999;
}

.recent > p
{
	width: 420px;
	margin: 0 auto 10px;
	font-size: 14px;
	color: #aaa;
}

.recent ul
{
	width: 400px;
    margin: 0 auto 30px;
}

.recent li
{
	padding: 5px 0;
	margin-bottom: 10px;
	cursor: pointer;
}


.recent li:hover
{
	font-weight: bold;
}

.wrap 
{
	width: 1100px;
	margin: 0 auto;
}

main
{
	width: 800px;
	min-height: 520px;
	margin: 80px auto 30px;
}

.main
{
	margin-top: 80px;
}
/*********************************************************** 쿠키 ***********************************************************/	
.cookies 
{
	display: block;
	width: 100%;
	height: 60px;
	background: #6667ab;
	
	position: fixed;
	bottom: 0;
	
	display: none;
}

/*********************************************************** 슬라이드 ***********************************************************/	
.slide 
{
	height:560px;
	background: #282828;	
}

.slide h2
{
	 width: 340px;
	 height: 60px;
	 line-height: 60px;
	 text-align: center;
	 font-size: 28px;
	 color: white;
	 margin: 0 auto;
}
.swiper 
{
	width: 80%;
	height: 360px;
}

.swiper-slide
{
	width: 260px;
	text-align: center;
	font-size: 18px;
	background: #fff;

	/* Center slide text vertically */
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
}

.swiper-slide a
{
	display: block;
	width: 100%;
	height: 100%;
}

.swiper-slide img
{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
/*********************************************************** 카테고리 ***********************************************************/	
.category 
{
	width: 100%;
	height: 60px;
	padding: 15px;
	text-align: center;
}

.category > a
{
	min-width: 120px;
