	margin-right: 0;
}

.user_recommend a,
.user_record a
{
	display: inline-block;
	width: 100%;
	height: 100%;
}

.modal_back
{
	display: none;
	z-index: 9999;
}

.modal
{
	padding: 20px;
	background: #fff;
	position: absolute;
	top: 35%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.modal h3
{
	text-align: center;
}

.modal input[type=password]
{
	width: 100%;
	height: 30px;
	border: 1px solid #ddd;
	padding: 5px 10px;
	margin-bottom: 15px;
}

.modal input[type=submit],
.modal input[type=button]
{
	border-radius: 10px;
	width: 100px;
}

.modal input[type=submit]
{
	background: #282828;
	color: #fff;
}

.modal input[type=submit]:hover
{
	background: #6667AB;
}

.modal input[type=button]:hover
{
	background: #ddd;
}

